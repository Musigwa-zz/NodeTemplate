{
  "name": "node-template",
  "version": "1.0.0",
  "description": "The NodeJs/Express application template to help you quick start your development.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "prettier": "eslint --ignore-path ./.gitignore . --fix"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongoose"
  ],
  "author": "MUSIGWA Pacifique",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "3.22.0",
    "http-status-codes": "1.4.0",
    "mongoose": "5.9.9",
    "morgan": "1.10.0",
    "nodemon": "2.0.3",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier"
    ]
  },
  "private": false,
  "devDependencies": {
    "husky": "4.2.5",
    "lint-staged": "10.1.5"
  }
}
